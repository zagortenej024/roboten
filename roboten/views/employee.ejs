<!-- views/employees.ejs -->

<!DOCTYPE html>
<html>
  <head>
    <title>Employees</title>
  </head>
  <body>
    <h1>Employees</h1>
    <table>
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% employees.forEach(function(employee) { %>
        <tr>
          <td><%= employee.firstName %></td>
          <td><%= employee.lastName %></td>
          <td><%= employee.email %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
